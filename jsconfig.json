{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@a/*": [
        "src/assets/*"
      ],
      "@c/*": [
        "src/components/*"
      ],
      "@l/*": [
        "src/layout/*"
      ],
      "@v/*": [
        "src/views/*"
      ],
      "@u/*": [
        "src/utils/*"
      ],
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  }
}
